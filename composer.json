{
    "name": "learnkit/entree-arp-service",
    "description": "This is my package entree-arp-service",
    "keywords": [
        "LearnKit",
        "laravel",
        "entree-arp-service"
    ],
    "homepage": "https://github.com/learnkit-dev/entree-arp-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Kloos",
            "email": "sebastiaan@codecycler.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "LearnKit\\EntreeArpService\\": "src",
            "LearnKit\\EntreeArpService\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LearnKit\\EntreeArpService\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LearnKit\\EntreeArpService\\EntreeArpServiceServiceProvider"
            ],
            "aliases": {
                "EntreeArpService": "LearnKit\\EntreeArpService\\EntreeArpServiceFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
